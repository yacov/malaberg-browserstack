{
    "require": {
        "friends-of-behat/mink-extension": "^2.7.5",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/page-object-extension": "^0.3",
        "behat/mink-selenium2-driver": "^1.6",
        "behat/behat": "^3.12"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "optimize-autoloader": true,
        "preferred-stable": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Page\\": "features/bootstrap/Page/",
            "": "lib/"
        }
    },
    "scripts": {
        "purchase": "vendor/bin/behat features/purchase.feature"
    },
    "require-dev": {
        "friends-of-behat/test-context": "^1.3"
    }
}
